{"ast":null,"code":"import { GET_INFRASTRUCTURES_URL } from '../config/ApiConfig';\nimport axios from 'axios';\nexport default {\n  name: 'Home',\n\n  data() {\n    return {\n      lstInfrastructure: null,\n      txtSearch: ''\n    };\n  },\n\n  methods: {\n    loadInfrastructures: function () {\n      axios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\n      axios.defaults.headers.common['Content-Type'] = 'application/json';\n      axios.get(GET_INFRASTRUCTURES_URL).then(response => {\n        this.lstInfrastructure = response.data;\n      });\n    },\n    findInfrastructure: function () {\n      //TO DO BASED ON FUNCTIONALITY\n      this.lstInfrastructure = this.lstInfrastructure.filter(x => x.structure_number.indexOf(this.txtSearch) > -1);\n    },\n    onChangeSearch: function (evt) {\n      this.txtSearch = evt.target.value;\n    }\n  },\n\n  mounted() {\n    this.loadInfrastructures();\n  }\n\n};","map":{"version":3,"mappings":"AAkBA,SAAQA,uBAAR,QAAsC,qBAAtC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,MADO;;EAEbC,IAAI,GAAG;IACL,OAAM;MACJC,iBAAiB,EAAE,IADf;MAEJC,SAAQ,EAAI;IAFR,CAAN;EAID,CAPY;;EAQbC,OAAO,EAAE;IACRC,mBAAmB,EAAE,YAAW;MAC/BN,KAAK,CAACO,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;MACET,KAAK,CAACO,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,cAA9B,IAA+C,kBAA/C;MACAT,KAAK,CAACU,GAAN,CAAUX,uBAAV,EAAmCY,IAAnC,CACEC,QAAO,IAAK;QACV,KAAKT,iBAAL,GAAyBS,QAAQ,CAACV,IAAlC;MACF,CAHF;IAKD,CATM;IAUPW,kBAAkB,EAAC,YAAU;MAC3B;MACA,KAAKV,iBAAL,GAAyB,KAAKA,iBAAL,CAAuBW,MAAvB,CAA+BC,CAAD,IAAMA,CAAC,CAACC,gBAAF,CAAmBC,OAAnB,CAA2B,KAAKb,SAAhC,IAA4C,CAAC,CAAjF,CAAzB;IACD,CAbM;IAcPc,cAAc,EAAC,UAASC,GAAT,EAAa;MAC1B,KAAKf,SAAL,GAAee,GAAG,CAACC,MAAJ,CAAWC,KAA1B;IACF;EAhBO,CARI;;EA2BbC,OAAO,GAAG;IACR,KAAKhB,mBAAL;EACC;;AA7BU,CAAf","names":["GET_INFRASTRUCTURES_URL","axios","name","data","lstInfrastructure","txtSearch","methods","loadInfrastructures","defaults","headers","common","get","then","response","findInfrastructure","filter","x","structure_number","indexOf","onChangeSearch","evt","target","value","mounted"],"sourceRoot":"","sources":["/Users/jquadros@unomaha.edu/nia/home/src/components/Home.vue"],"sourcesContent":["<template>\n<div class=\"container\">\n<input id=\"search\" type=\"text\" @change=\"onChangeSearch\" /> \n<button id=\"btnSearch\" @click=\"findInfrastructure()\">Search</button>\n<div class=\"items\">\n  <span>Structure Number</span>\n   <span>County Code</span>\n    <span>State Code</span>\n</div>\n<div class=\"items\" v-for=\"(item) in this.lstInfrastructure\">\n<span>{{item.structure_number}}</span>\n  <span>{{item.county_code}}</span>\n  <span>{{item.state_code}}</span>\n</div>\n\n</div>\n</template>\n<script>\nimport {GET_INFRASTRUCTURES_URL} from '../config/ApiConfig'\nimport axios from 'axios'\nexport default {\n  name: 'Home',\n  data() {\n    return{\n      lstInfrastructure: null,\n      txtSearch : ''\n    }\n  },\n  methods: {\n   loadInfrastructures: function() {\n    axios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\n      axios.defaults.headers.common['Content-Type'] ='application/json';\n      axios.get(GET_INFRASTRUCTURES_URL).then(\n        response => {\n          this.lstInfrastructure = response.data;\n        }\n      );\n    },\n    findInfrastructure:function(){\n      //TO DO BASED ON FUNCTIONALITY\n      this.lstInfrastructure = this.lstInfrastructure.filter((x)=> x.structure_number.indexOf(this.txtSearch) >-1 )\n    },\n    onChangeSearch:function(evt){\n      this.txtSearch=evt.target.value;\n    }\n    \n   },\n  mounted() {\n    this.loadInfrastructures();\n    },\n}\n</script>\n<style lang=\"scss\" scoped>\n\n.items\n{\n  display: flex;\n  flex-direction: row;\n  justify-content: space-evenly;\n}\n.container{\n  width: 100%;\n  height: 400px;\n    padding-right: 15px;\n    padding-left: 15px;\n    margin-right: auto;\n    margin-left: auto;\n  }\n\n</style>"]},"metadata":{},"sourceType":"module"}